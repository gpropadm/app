{
  "name": "crm-imobiliario",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3000",
    "build": "prisma generate && next build",
    "build:docker": "BUILD_STANDALONE=true next build",
    "start": "next start -p $PORT",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "docker:build": "docker build -t crm-imobiliario .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "vercel:deploy": "vercel deploy",
    "vercel:prod": "vercel deploy --prod",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@langchain/core": "^0.3.57",
    "@langchain/openai": "^0.5.12",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.9.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "langchain": "^0.3.27",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "node-fetch": "^2.7.0",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "tsx": "^4.19.4",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
