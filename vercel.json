{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "zeroConfig": true,
        "outputDirectory": ".next"
      }
    }
  ],
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "DISABLE_ESLINT": "1"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "DISABLE_ESLINT": "1"
    }
  }
}