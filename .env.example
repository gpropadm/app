DATABASE_URL="postgresql://user:password@localhost:5432/crm_imobiliario"

# Autenticação NextAuth
NEXTAUTH_SECRET="sua-chave-secreta-muito-longa-e-segura-aqui"
NEXTAUTH_URL="http://localhost:3000"

# OpenAI para Inteligência Artificial
OPENAI_API_KEY="sk-sua-chave-openai-aqui"

# Serviços de Notificação
EMAIL_SERVICE_API_KEY="sua-chave-do-servico-de-email"
WHATSAPP_API_KEY="sua-chave-whatsapp-business"
WHATSAPP_PHONE_NUMBER="+5511999999999"

# API de Boletos Bancários
BOLETO_API_KEY="sua-chave-api-boletos"
BOLETO_API_URL="https://api.provedor-boletos.com/v1"

# Asaas Gateway (Split de Pagamentos)
ASAAS_API_KEY="sua-chave-asaas-api"

# PJBank Gateway (Split de Pagamentos)
PJBANK_CREDENCIAL="sua-credencial-pjbank"
PJBANK_CHAVE="sua-chave-pjbank"

# Ambiente
NODE_ENV="development"

# URLs e Configurações
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="sua-chave-google-maps-api"

# Cache Redis (opcional)
REDIS_URL="redis://localhost:6379"

# Monitoramento (opcional)
SENTRY_DSN="sua-dsn-sentry-para-monitoramento"

# AgentQL - API Key para web scraping automático
AGENTQL_API_KEY="sua-chave-agentql-api-aqui"

# IPTU Services (para consultas automáticas de IPTU)
IPTU_SP_API_KEY="sua-chave-iptu-sao-paulo"
IPTU_RJ_API_KEY="sua-chave-iptu-rio-janeiro"
IPTU_MG_API_KEY="sua-chave-iptu-minas-gerais"

# Registry Services (para consultas automáticas de cartórios)
REGISTRY_SP_API_KEY="sua-chave-cartorio-sao-paulo"
REGISTRY_RJ_API_KEY="sua-chave-cartorio-rio-janeiro"

# Configurações AgentQL
ENABLE_AGENTQL_FEATURES=true
ENABLE_AUTO_SYNC=true
DEFAULT_SYNC_INTERVAL=60  # minutos
MAX_CONCURRENT_SCRAPING_JOBS=3
SCRAPING_TIMEOUT=300000  # milissegundos (5 minutos)

# Rate Limiting para scraping
AGENTQL_RATE_LIMIT_PER_MINUTE=10
PORTAL_REQUEST_DELAY=2000  # milissegundos entre requests